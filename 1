//find the subarray with largest sum in an array

#include <iostream>
using namespace std;

int main(){
    
    int i,j;
    int arr[] = {-2,1,-3,4,-1,2,1,-5,4};
    int n = 9;
    int temp = arr[0];
    for(i=0;i<n;i++){
        int sum =0;
        for(j=i;j<n;j++){
            sum = sum+arr[j];
        
        if(temp<sum){
            temp = sum;
          }
        }
    }
    cout<<temp;
    return 0;
}
